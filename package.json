{
  "name": "@bastionzero/zli",
  "version": "5.6.13",
  "description": "BastionZero cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastionzero/zli.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": "dist/src/index.js",
  "engines": {
    "node": "^14",
    "npm": ">=6"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build-binaries-macos": "tsc && cd bzero/bctl/daemon && GOOS=darwin GOARCH=amd64 go build -o daemon",
    "build-binaries-windows": "tsc && cd bzero/bctl/daemon && GOOS=windows GOARCH=amd64 go build -o daemon",
    "build-binaries-linux": "tsc && cd bzero/bctl/daemon && GOOS=linux GOARCH=amd64 go build -o daemon",
    "update-daemon-version": "replace '\\$DAEMON_VERSION' \"$(cat bzero/VERSION)\" bzero/bctl/daemon/daemon.go",
    "release-dev": "tsc && npm run package",
    "release": "run-script-os",
    "release:win32": "npm run clean && npm run release-windows",
    "release:linux": "npm run clean && npm run release-linux",
    "release:macos": "npm run clean && npm run release-macos",
    "release-all": "npm run clean && npm run release-macos && npm run release-linux && npm run release-windows",
    "release-macos": "npm run build-binaries-macos && npm run copy-files && pkg . -t node14-macos-x64 -o bin/zli-macos",
    "release-linux": "npm run build-binaries-linux && npm run copy-files && pkg . -t node14-linux-x64 -o bin/zli-linux",
    "release-windows": "npm run build-binaries-windows && npm run copy-files && pkg . -t node14-win-x64 -o bin/zli-win.exe",
    "release-prod": "npm run update-daemon-version && npm run release-all",
    "copy-files": "copyfiles src/**/*.html src/**/*.css bzero/bctl/daemon/* dist/",
    "package": "npm run copy-files && pkg .",
    "clean": "rimraf dist/ && rimraf bin/",
    "test": "jest --config jest-config-unit-tests.js",
    "system-test": "jest --config src/system-tests/jest-config-system-tests.js --forceExit --collectCoverage",
    "lint": "eslint 'src/**/*.ts' 'webshell-common-ts/**/*.ts' -c webshell-common-ts/.eslintrc.json"
  },
  "author": "nabeelperson",
  "license": "Apache-2.0",
  "devDependencies": {
    "axios": "^0.26.0",
    "@kubernetes/client-node": "^0.16.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "copyfiles": "^2.4.1",
    "dots-wrapper": "^3.7.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-unused-imports": "^1.1.0",
    "jest": "^27.3.1",
    "mock-stdin": "^1.0.0",
    "pg": "^8.7.3",
    "pkg": "^5.3.1",
    "replace": "^1.2.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.0.5",
    "wait-for-expect": "^3.0.2"
  },
  "dependencies": {
    "@jsier/retrier": "^1.2.4",
    "@microsoft/signalr": "^5.0.0",
    "@types/async": "^3.2.5",
    "@types/chalk": "^2.2.0",
    "@types/crypto-js": "^4.0.1",
    "@types/figlet": "^1.2.1",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.7",
    "@types/prompts": "^2.0.14",
    "@types/qrcode": "^1.3.5",
    "@types/semver": "^7.3.4",
    "@types/ssh2": "^0.5.48",
    "@types/sshpk": "^1.10.7",
    "@types/tcp-port-used": "^1.0.0",
    "@types/term-size": "^2.0.3",
    "@types/yargs": "^17.0.5",
    "async": "^3.2.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.0",
    "conf": "^7.1.2",
    "crypto-js": "^4.0.0",
    "figlet": "^1.5.0",
    "find-open-port": "^2.0.3",
    "form-data": "^3.0.0",
    "got": "^11.8.0",
    "is-running": "^2.1.0",
    "libp2p-spdy": "^0.13.3",
    "lodash": "^4.17.20",
    "mixpanel": "^0.13.0",
    "noble-ed25519": "^1.0.3",
    "open": "^7.3.0",
    "openid-client": "^4.7.2",
    "ora": "5.4.1",
    "port-pid": "^0.0.7",
    "prompt": "^1.2.0",
    "qrcode": "^1.3.2",
    "query-string": "^7.0.1",
    "rand-token": "^1.0.1",
    "read-last-lines": "^1.8.0",
    "rxjs": "^6.6.3",
    "semver": "^7.3.2",
    "sha3": "^2.1.4",
    "ssh-config": "^4.0.6",
    "ssh2": "^1.4.0",
    "ssh2-promise": "^1.0.1",
    "sshpk": "^1.16.1",
    "tcp-port-used": "^1.0.2",
    "term-size": "^2.2.1",
    "tmp": "^0.2.1",
    "tree-kill": "^1.2.2",
    "ts-node": "^9.0.0",
    "universal-analytics": "^0.5.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^7.5.1",
    "yargs": "^17.2.1"
  },
  "pkg": {
    "scripts": [],
    "assets": [
      "node_modules/figlet/fonts/Standard.flf",
      "bzero/bctl/daemon/daemon"
    ],
    "outputPath": "bin",
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64"
    ]
  }
}
