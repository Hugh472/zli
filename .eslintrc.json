{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        // "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": [
    ],
    "rules": {
        "indent": ["error", 4],
        "no-trailing-spaces": ["error"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }
}
